
@{
    ViewData["Title"] = "Shippers";
}

<h1>Shippers</h1>
<button class="btn btn-primary" id="button1">Get Shippers</button>

@section Scripts{
    <script>
        const btn = document.querySelector('#button1');
        btn.addEventListener('click', async () => {
            const response = await fetch('https://localhost:7112/api/Shippers/1')
            const data = await response.json()
            console.log(data)
        })
    </script>

}